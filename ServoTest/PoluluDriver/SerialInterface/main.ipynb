    "import serial\n",
    "\n",
    "ser = serial.Serial('COM12', 9600)\n",
    "command=b'\\xa7'\n",
    "subroute=b'\\x00'\n",
    "ser.write(command)\n",
    "ser.write(subroute)\n",
    "\n",
    "ser.close()"
